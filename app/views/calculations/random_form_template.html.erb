<h1>Random Calculation</h1>

<form action="/random/results">
    
  <div>
    <label>
        Minimum
    </label>
    
    <input id="number_input_low" type="text" name="low_bound" placeholder="E.g. 1.5">
  </div>
  
  
  <div>
    <label>
        Maximum
    </label>
    
    <input id="number_input_high" type="text" name="high_bound" placeholder="E.g. 4.5">
  </div>
  
  
  <button>Pick random number</button>
</form>